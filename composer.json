{
    "name": "reveal-ui/framework",
    "description": "Laravel Components",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "anhskohbo/no-captcha": "^3.0",
        "illuminate/support": "5.*",
        "spatie/laravel-analytics": "^3.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/shawnsandy/backstory"
        },
        {
            "type": "vcs",
            "url": "https://github.com/shawnsandy/img-fly"
        },
        {
            "type": "vcs",
            "url": "https://github.com/shawnsandy/jarvis"
        },
        {
            "type": "vcs",
            "url": "https://github.com/shawnsandy/dash-auth"
        }
    ],
    "author": {
        "name": "shawn sandy ",
        "email": "shawnsandy04@gmail.com"
    },
    "autoload": {
        "psr-4": {
            "Reveal\\Framework\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reveal\\Framework\\FrameworkServiceProvider",
                "Spatie\\Analytics\\AnalyticsServiceProvider",
                "Anhskohbo\\NoCaptcha\\NoCaptchaServiceProvider"
            ],
            "aliases": {
                "Analytics": "Spatie\\Analytics\\AnalyticsFacade",
                "NoCaptcha": "Anhskohbo\\NoCaptcha\\Facades\\NoCaptcha"
            }
        }
    },
    "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
